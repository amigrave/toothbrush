*customization: -color
XTerm*termName:  xterm-256color
URxvt.termName:  rxvt-256color

URxvt*geometry:160x40
!URxvt*transparent:true
!URxvt*shading:15

! to match gnome-terminal "Linux console" scheme
! foreground/background
URxvt*background: #000000
URxvt*foreground: #ffffff

XTerm*background: #000000
XTerm*foreground: #ffffff
! black
!URxvt.color0  : #000000
URxvt.color0  : #2E3436
!URxvt.color8  : #555555
URxvt.color8  : #555753
! red
!URxvt.color1  : #AA0000
URxvt.color1  : #CC0000
!URxvt.color9  : #FF5555
URxvt.color9  : #EF2929
! green
!URxvt.color2  : #00AA00
URxvt.color2  : #4E9A06
!URxvt.color10 : #55FF55
URxvt.color10 : #8AE234
! yellow
!URxvt.color3  : #AA5500
URxvt.color3  : #C4A000
!URxvt.color11 : #FFFF55
URxvt.color11 : #FCE94F
! blue
!URxvt.color4  : #0000AA
URxvt.color4  : #3465A4
!URxvt.color12 : #5555FF
URxvt.color12 : #729FCF
! magenta
!URxvt.color5  : #AA00AA
!URxvt.color5  : #75507B
URxvt.color5  : #995099
!URxvt.color13 : #FF55FF
!URxvt.color13 : #AD7FA8
URxvt.color13 : #CC7FCC
! cyan
!URxvt.color6  : #00AAAA
URxvt.color6  : #06989A
!URxvt.color14 : #55FFFF
URxvt.color14 : #34E2E2
! white
!URxvt.color7  : #AAAAAA
URxvt.color7  : #D3D7CF
!URxvt.color15 : #FFFFFF
URxvt.color15 : #EEEEEC

URxvt.background: black
URxvt.foreground: #cccccc

!URxvt.font: -misc-console-*-r-*--16-160-72-72-c-80-*-1,fixed
!xterm*font:-bitstream-terminal-bold-r-*-*-*-140-100-100-*-*-iso8859-1
!URxvt*font: -*-terminus-*-*-*-*-14-*-*-*-*-*-*-*
URxvt*font: xft:Monospace:pixelsize=12
URxvt*boldFont: xft:Monospace:bold:pixelsize=12
URxvt*letterSpace: -1

URxvt.perl-ext: default,matcher,tabbedex,-searchable-scrollback,-readline
URxvt.urlLauncher: google-chrome
URxvt.matcher.button: 1

URxvt.tabbed.tabbar-fg: 7
URxvt.tabbed.tabbar-bg: 4
URxvt.tabbed.tab-fg: 15
URxvt.tabbed.tab-bg: 2

URxvt.saveLines: 2000
URxvt.scrollBar_right: true
URxvt.scrollColor: #99AA99
URxvt*scrollstyle:rxvt
URxvt.cursorBlink:	true
!URxvt.title:	false

*Paned.Text.translations:	#override\n\
 Shift<Btn4Down>,<Btn4Up>: scroll-one-line-down()\n\
 Shift<Btn5Down>,<Btn5Up>: scroll-one-line-up()\n\
 Ctrl<Btn4Down>,<Btn4Up>: previous-page()\n\
 Ctrl<Btn5Down>,<Btn5Up>: next-page()\n\
 None<Btn4Down>,<Btn4Up>:scroll-one-line-down()scroll-one-line-down()scroll-one-line-down()scroll-one-line-down()scroll-one-line-down()\n\
 None<Btn5Down>,<Btn5Up>:scroll-one-line-up()scroll-one-line-up()scroll-one-line-up()scroll-one-line-up()scroll-one-line-up()\n
