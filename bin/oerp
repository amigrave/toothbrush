#!/bin/sh
(iselect -a | sh -x) <<EOS
# CLIENT 5.0
cd $HOME/openerp/source/client/5.0/bin; ./openerp-client.py &
cd $HOME/openerp/source/client/5.0/bin; ./openerp-client.py -s localhost -u admin &
cd $HOME/openerp/source/client/5.0/bin; ./openerp-client.py -s tiny.my.odoo.com -u fme &
cd $HOME/openerp/source/client/5.0/bin; ./openerp-client.py -s agrplus.com -u admin &
cd $HOME/openerp/source/client/5.0/bin; ./openerp-client.py -s cc.odoo.com -u fme &
cd $HOME/openerp/source/client/5.0/bin; ./openerp-client.py -s cc.test.odoo.com -u fme &
cd $HOME/openerp/source/client/5.0/bin; ./openerp-client.py -s cc.my.openerp.com -u fme &

# CLIENT TRUNK
cd $HOME/openerp/client-trunk/bin; ./openerp-client.py &
cd $HOME/openerp/client-trunk/bin; ./openerp-client.py -s localhost -u admin -l debug_rpc &
cd $HOME/openerp/source/client/trunk/bin; ./openerp-client.py -s openerp.my.odoo.com -u fme &

# SERVERS
cd $HOME/openerp/server/bin; ./openerp-server.py
cd $HOME/openerp/server/bin; ./openerp-server.py -u odms -d cc --enable-code-actions --log-level=debug
cd $HOME/openerp/server-trunk/bin; ./openerp-server.py -c ~/.openerp_serverrc_trunk

# WEB
$HOME/openerp/web/openerp-web.py
$HOME/openerp/web-trunk/openobject-web.py

# SSH
ssh root@delta.openerp.com
ssh root@evaluation-matrix.com # Old server that's gonna DIE !
EOS
