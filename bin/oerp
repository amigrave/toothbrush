# CLIENT 5.0
cd /home/amigrave/dev/openerp/bazaar/client-5.0/bin; ./openerp-client.py &
cd /home/amigrave/dev/openerp/bazaar/client-5.0/bin; ./openerp-client.py -s localhost -u admin &
cd /home/amigrave/dev/openerp/bazaar/client-5.0/bin; ./openerp-client.py -s tiny.my.odoo.com -u fme &
cd /home/amigrave/dev/openerp/bazaar/client-5.0/bin; ./openerp-client.py -s agrplus.com -u admin &
cd /home/amigrave/dev/openerp/bazaar/client-5.0/bin; ./openerp-client.py -s cc.odoo.com -u fme &
cd /home/amigrave/dev/openerp/bazaar/client-5.0/bin; ./openerp-client.py -s cc.test.odoo.com -u fme &
cd /home/amigrave/dev/openerp/bazaar/client-5.0/bin; ./openerp-client.py -s cc.my.openerp.com -u fme &

# CLIENT TRUNK
